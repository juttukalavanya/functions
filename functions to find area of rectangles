// area of rectangle using functions 
#include <stdio.h>
 int areaofrect(int length , int breath)
{
    int area;
    area = length * breath;
    return area;
}
int main()
{
    int l=10, b=5;
    int area= areaofrect(l,b);
    printf("%d\n",area);
{
l=50,b=20;
area=areaofrect(l,b);
printf("%d",area);
}
}

 output:
 50
1000
